#include <iostream>

using namespace std;

int main()
{   int n;
    string texto,textoinver;
    getline(cin,texto);
    bool band=1;
    n=texto.length();
    for(int i=n-1,j=0;i>=0,j<n;i--,j++){
        textoinver[j]=texto[i];
    }
    for(int a=0,b=0;a<n,b<n;a++,b++){
        if(textoinver[a]!=texto[b]){
            band=0;
            break;
        }
    }
   if(band==1){
    cout<<"Si"<<endl;
   }
   else{
    cout<<"No"<<endl;
   }


    return 0;
}
